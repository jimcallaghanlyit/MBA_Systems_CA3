  <DataSets>
    <DataSet Name="Enrolment_Year">
      <Query>
        <DataSourceName>MBASystemsCA3SharedDataSource</DataSourceName>
        <CommandText>SELECT DATEPART(YEAR, Student.StudentEnrolDate) AS 'Year', COUNT(Student.StudentID) AS TotalEnrolments
FROM
  Student
GROUP BY DATEPART(YEAR, Student.StudentEnrolDate)
ORDER BY 'YEAR'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
