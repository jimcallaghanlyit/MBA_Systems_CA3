      <Query>
        <DataSourceName>MBASystemsCA3SharedDataSource</DataSourceName>
        <CommandText>SELECT
  School.SchoolName
  ,Accredsts.AccredStsYears
  ,Accredsts.SchoolAccredSts
  ,School.AccredID
FROM
  Accredsts
  INNER JOIN School
    ON Accredsts.AccredID = School.AccredID
WHERE Accredsts.AccredID = '1'</CommandText>
